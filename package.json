{
    "name": "search-on",
    "publisher": "saxc",
    "displayName": "Search On ...",
    "description": "Search On, is an editor extension that allows developers to easily search selected text across multiple platforms. With this extension, you can quickly access information from websites like Google, GitHub, MDN, and many others.",
    "version": "25.11.1",
    "engines": {
        "vscode": "^1.100.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/saxc/search-on"
    },
    "license": "MIT",
    "icon": "images/logo.drawio.png",
    "categories": [
        "Other"
    ],
    "keywords": [
        "Search",
        "Documentation",
        "Customizable",
        "Bing",
        "Brave",
        "crates.io",
        "CSS-Tricks",
        "devdocs.io",
        "dict.cc",
        "Docker Hub",
        "Docs.rs",
        "DuckDuckGo",
        "Ecosia",
        "git",
        "GitHub",
        "Google Translator",
        "Google",
        "Kotlin",
        "MDN",
        "Microsoft Learn",
        "NPM",
        "NuGet",
        "PyPI",
        "Python",
        "Reddit",
        "StackOverflow",
        "Startpage",
        "Visual Studio Code",
        "VS Code",
        "Wikipedia",
        "YouTube"
    ],
    "activationEvents": [],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "search-on.search",
                "title": "Search On ...",
                "category": "Search on"
            },
            {
                "command": "search-on.openSettings",
                "title": "Open settings",
                "category": "Search on"
            },
            {
                "command": "search-on.enablePlatform",
                "title": "Enable platform",
                "category": "Search on"
            },
            {
                "command": "search-on.disablePlatform",
                "title": "Disable platform",
                "category": "Search on"
            }
        ],
        "menus": {
            "editor/context": [
                {
                    "when": "editorHasSelection",
                    "command": "search-on.search",
                    "group": "navigation"
                }
            ]
        },
        "configuration": {
            "type": "object",
            "title": "Search On ...",
            "properties": {
                "searchOn.customPlatforms": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "properties": {
                            "label": {
                                "type": "string",
                                "description": "Name of the custom platform."
                            },
                            "url": {
                                "type": "string",
                                "description": "URL for the custom platform."
                            }
                        },
                        "required": [
                            "label",
                            "url"
                        ]
                    },
                    "default": [],
                    "description": "Add your own platforms. See the README for further details."
                },
                "searchOn.bing": {
                    "type": "boolean",
                    "default": false,
                    "description": "Search on Bing"
                },
                "searchOn.brave": {
                    "type": "boolean",
                    "default": false,
                    "description": "Search on Brave"
                },
                "searchOn.cratesIo": {
                    "type": "boolean",
                    "default": false,
                    "description": "Search on crates.io"
                },
                "searchOn.cssTricks": {
                    "type": "boolean",
                    "default": false,
                    "description": "Search on CSS-Tricks"
                },
                "searchOn.devdocs": {
                    "type": "boolean",
                    "default": false,
                    "description": "Search on devdocs.is"
                },
                "searchOn.dictCc": {
                    "type": "boolean",
                    "default": false,
                    "description": "Search on dict.cc"
                },
                "searchOn.docsRs": {
                    "type": "boolean",
                    "default": false,
                    "description": "Search on Docs.rs"
                },
                "searchOn.dockerHub": {
                    "type": "boolean",
                    "default": true,
                    "description": "Search on Docker Hub"
                },
                "searchOn.duckDuckGo": {
                    "type": "boolean",
                    "default": false,
                    "description": "Search on DuckDuckGo"
                },
                "searchOn.ecosia": {
                    "type": "boolean",
                    "default": false,
                    "description": "Search on Ecosia"
                },
                "searchOn.git": {
                    "type": "boolean",
                    "default": false,
                    "description": "Search on git"
                },
                "searchOn.gitHub": {
                    "type": "boolean",
                    "default": true,
                    "description": "Search on GitHub"
                },
                "searchOn.google": {
                    "type": "boolean",
                    "default": true,
                    "description": "Search on Google"
                },
                "searchOn.googleTranslator": {
                    "type": "boolean",
                    "default": false,
                    "description": "Search on Google Translator"
                },
                "searchOn.kotlin": {
                    "type": "boolean",
                    "default": false,
                    "description": "Search on Kotlin"
                },
                "searchOn.MDN": {
                    "type": "boolean",
                    "default": true,
                    "description": "Search on MDN (Mozilla Developer Network)"
                },
                "searchOn.microsoftLearn": {
                    "type": "boolean",
                    "default": true,
                    "description": "Search on Microsoft Learn"
                },
                "searchOn.NPM": {
                    "type": "boolean",
                    "default": true,
                    "description": "Search on NPM (Node Package Manager)"
                },
                "searchOn.nuGet": {
                    "type": "boolean",
                    "default": true,
                    "description": "Search on NuGet"
                },
                "searchOn.pyPI": {
                    "type": "boolean",
                    "default": false,
                    "description": "Search on PyPI"
                },
                "searchOn.python": {
                    "type": "boolean",
                    "default": false,
                    "description": "Search on Python documentation"
                },
                "searchOn.reddit": {
                    "type": "boolean",
                    "default": false,
                    "description": "Search on Reddit"
                },
                "searchOn.stackOverflow": {
                    "type": "boolean",
                    "default": true,
                    "description": "Search on Stack Overflow"
                },
                "searchOn.startpage": {
                    "type": "boolean",
                    "default": false,
                    "description": "Search on Startpage"
                },
                "searchOn.vs-code": {
                    "type": "boolean",
                    "default": false,
                    "description": "Search on VS Code"
                },
                "searchOn.wikipedia": {
                    "type": "boolean",
                    "default": true,
                    "description": "Search on Wikipedia"
                },
                "searchOn.youTube": {
                    "type": "boolean",
                    "default": true,
                    "description": "Search on YouTube"
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src",
        "test": "vscode-test"
    },
    "devDependencies": {
        "@types/vscode": "^1.100.0",
        "@types/mocha": "^10.0.9",
        "@types/node": "20.x",
        "@typescript-eslint/eslint-plugin": "^8.10.0",
        "@typescript-eslint/parser": "^8.7.0",
        "eslint": "^9.13.0",
        "typescript": "^5.6.3",
        "@vscode/test-cli": "^0.0.10",
        "@vscode/test-electron": "^2.4.1"
    }
}